from typing import IO, Any, Literal

class YAMLError(Exception): ...

class YAML:
    default_flow_style: bool | None

    def __init__(self, *, typ: Literal["rt", "safe", "unsafe", "base"] | None = None) -> None: ...
    def load(self, stream: IO[str] | str) -> Any: ...
    def dump(
        self, data: Any, stream: IO[str] | None = None, *, transform: Any = None
    ) -> str | None: ...
